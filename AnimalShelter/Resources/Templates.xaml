<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:AnimalShelter.Views"
                    xmlns:viewModels="clr-namespace:AnimalShelter.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:DogsViewModel}">
        <views:DogsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:SheltersViewModel}">
        <views:SheltersView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:FilterViewModel}">
        <views:FilterView/>
    </DataTemplate>
</ResourceDictionary>